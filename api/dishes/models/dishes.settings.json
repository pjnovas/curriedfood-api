{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "dishes",
  "info": {
    "name": "Dishes"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "recipe": {
      "type": "richtext"
    },
    "photo": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "servings": {
      "type": "integer"
    },
    "ingredients": {
      "type": "component",
      "repeatable": true,
      "component": "products.product-item"
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "dishes"
    },
    "duration": {
      "type": "integer",
      "min": 0
    }
  }
}
